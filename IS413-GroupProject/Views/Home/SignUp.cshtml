@{
    ViewData["Title"] = "Available Appointments";
}
<h2 class="display-4">@ViewData["Title"]</h2>

@{
    ViewData["Title"] = "Home Page";
}

@model IS413_GroupProject.Models.ViewModels.TourListViewModel
<!--This loop iterates through the model and gets all of the information that was entered in and adds it to the output-->

@foreach (var x in Model.Tours)
{
    <partial name="DateCardSummary" model="x" />
}

<div page-model="@Model.PagingInfo" page-action="SignUp" page-classes-enabled="true" page-class=" btn"
     page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary"
     class="btn-group pull-right m-1"></div>

